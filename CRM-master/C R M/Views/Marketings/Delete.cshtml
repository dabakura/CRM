@model C_R_M.Models.Marketing

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Eliminar</h2>


<div>
    <h4>Marketing</h4>
    <h3>¿Está seguro que desea eliminar el registro?</h3>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("URL")
        </dt>
        <dd>
            @Html.DisplayFor(model => model.URL)
        </dd>
        <dt>
            @Html.DisplayName("Empresa")
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Empresa.Nombre)
        </dd>
        <dt>
            @Html.DisplayName("Producto Comprado")
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Producto.Nombre)
        </dd>
        <dt>
            @Html.DisplayName("Producto Sugerido")
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Producto1.Nombre)
        </dd>
    </dl>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Eliminar" class="btn btn-default" /> |
            @Html.ActionLink("Volver a la lista", "Index")
        </div>
    }
</div>
