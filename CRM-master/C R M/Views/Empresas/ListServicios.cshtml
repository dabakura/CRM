@model IEnumerable<C_R_M.Models.ServicioEmpresa>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha_Creacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Primer_Pago)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Renovacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Precio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Empresa.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Producto.Nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha_Creacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Primer_Pago)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Renovacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Precio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Empresa.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Producto.Nombre)
        </td>
        <td>
            <a class="btn btn-info ml-1" href="@Url.Action("Details","ServicioEmpresas",null)/@item.Id_Servicio_Empresa"><i class="fa fa-info-circle"></i></a>
            <a class="btn btn-success ml-1" href="@Url.Action("Edit","ServicioEmpresas",null)/@item.Id_Servicio_Empresa"><i class="fa fa-edit  fa-lg"></i></a>
        </td>
    </tr>
}

</table>
